{
    "name": "<owner>/<repo>",
    "description": "<description>",
    "keywords": ["<keywords>"],
    "homepage": "<homepage>",
    "type": "<library type>",
    "license": "MIT",
    "authors": [
        {
            "name": "<owner name>",
            "homepage": "<owner url>",
            "role": "Owner"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "dist": "*"
        }
    },
    "readme": ".github/README.md",
    "support": {
        "issues": "https://github.com/<Owner>/<Repo>/issues",
        "source": "https://github.com/<Owner>/<Repo>"
    },
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "spatie/phpunit-watcher": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "<Owner>\\<Repo>\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "<Owner>\\<Repo>\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-next": "1.0.x-dev"
        }
    }
}
