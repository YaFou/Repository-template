{
    "name": "<owner>/<repo>",
    "description": "<description>",
    "keywords": ["<keywords>"],
    "homepage": "<homepage>",
    "type": "<library type>",
    "license": "MIT",
    "authors": [
        {
            "name": "<owner name>",
            "homepage": "<owner url>",
            "role": "Owner"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "dist": "*"
        }
    },
    "readme": ".github/README.md",
    "support": {
        "issues": "https://github.com/<owner>/<repo>/issues",
        "source": "https://github.com/<owner>/<repo>"
    },
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "spatie/phpunit-watcher": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "<owner>\\<repo>\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "<owner>\\<repo>\\Tests\\": "tests/"
        }
    }
}